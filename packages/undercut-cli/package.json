{
  "name": "@undercut/cli",
  "version": "0.4.0",
  "description": "Undercut CLI: process data with undercut and JS in a terminal",
  "author": "Anton Alexandrenok",
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "bin": {
    "undercut": "cli.js"
  },
  "engines": {
    "node": ">=13.2"
  },
  "scripts": {
    "build": ". scripts/build.sh",
    "clear": "rm -rf build dist",
    "dist": ". scripts/dist.sh",
    "prepack": "test -e pull.js",
    "test": "jest --ci --config jest.config.json --rootDir build"
  },
  "dependencies": {
    "core-js": "^3.4.2",
    "@undercut/pull": "^0.4.0",
    "@undercut/push": "^0.4.0",
    "@undercut/utils": "^0.4.0",
    "yargs": "^15.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/the-spyke/undercut",
    "directory": "packages/undercut-cli"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "cli.js",
    "index.js",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "cli",
    "data",
    "filter",
    "pipe",
    "pipeline",
    "stdin",
    "stdout",
    "undercut",
    "util"
  ]
}
